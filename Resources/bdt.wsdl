                    <xsd:element name="StartDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="EndDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="IgnoreEndDate" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="TrailingPeriod" type="bdt:EnumTrailingPeriod" minOccurs="0" maxOccurs="1"/>
