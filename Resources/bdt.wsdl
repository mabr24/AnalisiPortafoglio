                    <xsd:element name="StressSimulation" type="bdt:StressSimulation" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="VAR" type="bdt:VAR" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="VarBackTesting" type="bdt:VarBackTesting" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="Performance" type="bdt:Performance" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="MCVar" type="bdt:VAR" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="ColAssetVal" type="bdt:ColAssetVal" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="ReturnCalculation" type="bdt:ReturnCalculation" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="UserDataExport" type="bdt:UserDataExport" minOccurs="0" maxOccurs="1"/>
