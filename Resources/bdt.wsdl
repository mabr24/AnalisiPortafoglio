                        <xsd:element name="Client" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Password" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="ExportSetName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="ScheduleExportJob" type="bdt:ScheduleExportJob" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
