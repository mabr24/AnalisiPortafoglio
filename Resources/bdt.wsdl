                <xsd:attribute name="BonusLevel" type="xsd:double" use="required"/>
                <xsd:attribute name="Cap" type="xsd:double" use="optional"/>
                <xsd:attributeGroup ref="bdt:BaseInstrumentIdentifier"/>
