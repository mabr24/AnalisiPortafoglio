            <xsd:complexType name="RiskBudgetingConstraints">
                <xsd:sequence>
                    <xsd:element name="RiskBudgetingConstraint1" type="bdt:RiskBudgetingConstraint" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="RiskBudgetingConstraint2" type="bdt:RiskBudgetingConstraint" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="RiskBudgetingConstraint3" type="bdt:RiskBudgetingConstraint" minOccurs="0" maxOccurs="1"/>
