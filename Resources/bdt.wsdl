                    <xsd:element name="ContentType" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="BinaryData" type="bdt:Base64Binary2ADB" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
