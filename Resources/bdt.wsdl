            <xsd:simpleType name="StorageMode">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="NONE"/>
                    <xsd:enumeration value="REFRESH"/>
                    <xsd:enumeration value="POPULATE"/>
                    <xsd:enumeration value="DELETE"/>
                </xsd:restriction>
