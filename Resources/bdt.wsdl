            <xsd:complexType name="ColAssetVal">
                <xsd:sequence>
                    <xsd:element name="PortfolioSelection" type="bdt:PortfolioSelection" minOccurs="1" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="Collateralized" type="xsd:boolean" use="optional"/>
                <xsd:attribute name="StructuredAsset" type="xsd:boolean" use="optional"/>
                <xsd:attribute name="ModelName" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelOwner" type="xsd:string" use="optional"/>
            </xsd:complexType>

            <xsd:complexType name="PortfolioExposure">
                <xsd:sequence>
                    <xsd:element name="PortfolioSelection" type="bdt:PortfolioSelection" minOccurs="1" maxOccurs="unbounded"/>
                    <xsd:element name="ReportSelection" type="bdt:ReportSelection" minOccurs="0" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute name="ModelName" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelOwner" type="xsd:string" use="optional"/>
                <xsd:attribute name="CovarianceDate" type="xsd:date" use="optional"/>
            </xsd:complexType>

            <xsd:complexType name="PortfolioAnalysis">
                <xsd:sequence>
                    <xsd:element name="PortfolioSelection" type="bdt:PortfolioSelection" minOccurs="1" maxOccurs="unbounded"/>
                    <xsd:element name="ReportSelection" type="bdt:ReportSelection" minOccurs="1" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="RecognizedAssets" type="xsd:int" use="optional"/>
                <xsd:attribute name="ModelName" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelOwner" type="xsd:string" use="optional"/>
                <xsd:attribute name="CovarianceDate" type="xsd:date" use="optional"/>
            </xsd:complexType>

            <xsd:complexType name="PortfolioSelection">
                <xsd:sequence>
                    <xsd:element name="PortfolioName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="PortfolioOwner" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="BenchmarkName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="BenchmarkOwner" type="xsd:string" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="IncludeAggregate" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="AggregationType" type="bdt:NodeSelectionInfo" minOccurs="0" maxOccurs="1" default="NONE"/>
                    <xsd:element name="SpecificLevel" type="xsd:string" minOccurs="0" maxOccurs="1" default=""/>
                    <xsd:element name="StorageEnabled" type="xsd:boolean" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="Currency" type="xsd:string" minOccurs="0" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="MPC">
                <xsd:sequence>
                    <xsd:element name="MPCCaseSelection" type="bdt:MPCCaseSelection" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="ReportSelection" type="bdt:ReportSelection" minOccurs="1" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="ModelName" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelOwner" type="xsd:string" use="optional"/>
                <xsd:attribute name="CovarianceDate" type="xsd:date" use="optional"/>
            </xsd:complexType>

            <xsd:complexType name="MPCCaseSelection">
                <xsd:sequence>
                    <xsd:element name="Name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="Owner" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="StressSimulation">
                <xsd:sequence>
                    <xsd:element name="ScenarioSelection" type="bdt:ScenarioSelection" minOccurs="1" maxOccurs="unbounded"/>
                    <xsd:element name="PortfolioSelection" type="bdt:PortfolioSelection" minOccurs="1" maxOccurs="unbounded"/>
                    <xsd:element name="ReportSelection" type="bdt:ReportSelection" minOccurs="1" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="Currency" type="xsd:string" use="required"/>
                <xsd:attribute name="ModelName" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelOwner" type="xsd:string" use="optional"/>
                <xsd:attribute name="CovarianceDate" type="xsd:date" use="optional"/>
            </xsd:complexType>

            <xsd:complexType name="ScenarioSelection">
                <xsd:sequence>
                    <xsd:element name="Name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="Owner" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="ModelName" type="xsd:string" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="ModelOwner" type="xsd:string" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="CovarianceDate" type="xsd:date" minOccurs="0" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="VAR">
                <xsd:sequence>
                    <xsd:element name="VarProfileSelection" type="bdt:VarProfileSelection" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="PortfolioSelection" type="bdt:PortfolioSelection" minOccurs="1" maxOccurs="unbounded"/>
                    <xsd:element name="ReportSelection" type="bdt:ReportSelection" minOccurs="1" maxOccurs="unbounded"/>
                </xsd:sequence>
                <xsd:attribute name="Currency" type="xsd:string" use="required"/>
                <xsd:attribute name="ModelName" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelOwner" type="xsd:string" use="optional"/>
                <xsd:attribute name="GenerateAssetLevelReports" type="xsd:boolean" use="optional"/>
                <xsd:attribute name="AssetLevelReportFileSizeMB" type="xsd:int" use="optional"/>
                <xsd:attribute name="CovarianceDate" type="xsd:date" use="optional"/>
            </xsd:complexType>

            <xsd:complexType name="VarProfileSelection">
                <xsd:sequence>
                    <xsd:element name="Name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="Owner" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="VarBackTesting">
                <xsd:sequence>
                    <xsd:element name="VarProfileSelection" type="bdt:VarProfileSelection" minOccurs="0" maxOccurs="unbounded"/>
                    <xsd:element name="PortfolioSelection" type="bdt:PortfolioSelection" minOccurs="1" maxOccurs="unbounded"/>
                    <xsd:element name="BackTestingPeriod" type="bdt:BackTestingPeriod" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute name="Currency" type="xsd:string" use="required"/>
                <xsd:attribute name="HoldingPeriodDay" type="xsd:int" use="required"/>
                <xsd:attribute name="ReturnSource" type="xsd:string" use="optional"/>
                <xsd:attribute name="ReturnSourceOwner" type="xsd:string" use="optional"/>
                <xsd:attribute name="GenerateAssetLevelReports" type="xsd:boolean" use="optional"/>
                <xsd:attribute name="AssetLevelReportFileSizeMB" type="xsd:int" use="optional"/>
                <xsd:attribute name="ModelName" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelOwner" type="xsd:string" use="optional"/>
            </xsd:complexType>

            <xsd:complexType name="BackTestingPeriod">
                <xsd:sequence>
                    <xsd:element name="PeriodUnit" type="bdt:EnumBackTestingPeriod" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="PeriodValue" type="xsd:int" minOccurs="0"  maxOccurs="1"/>  <!-- only applicable when unit is Day.-->
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="Performance">
                <xsd:sequence>
                    <xsd:element name="PerformanceSelection" type="bdt:PerformanceSelection" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="PortfolioSelection" type="bdt:PortfolioSelection" minOccurs="1" maxOccurs="unbounded"/>
                    <xsd:element name="StorageMode" type="bdt:StorageMode" minOccurs="0" maxOccurs="1" default="NONE"/>
                    <xsd:element name="PAStorageInvalidation" type="bdt:PAStorageInvalidation" minOccurs="0" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute name="Currency" type="xsd:string" use="required"/>
                <xsd:attribute name="ReturnSource" type="xsd:string" use="optional"/>
                <xsd:attribute name="ReturnSourceOwner" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelName" type="xsd:string" use="optional"/>
                <xsd:attribute name="ModelOwner" type="xsd:string" use="optional"/>
                <xsd:attribute name="PAStorageType" type="bdt:PAStorageType" use="optional" default="RESULTS"/>
            </xsd:complexType>

            <xsd:complexType name="PAStorageInvalidation">
                <xsd:attribute name="StartDate" type="xsd:date" use="required"/>
                <xsd:attribute name="EndDate" type="xsd:date" use="required"/>
            </xsd:complexType>

            <xsd:complexType name="PerformanceSelection">
                <xsd:sequence>
                    <xsd:element name="Name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="Owner" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:complexType name="ReportSelection">
                <xsd:sequence>
                    <xsd:element name="Name" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="Owner" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:complexType>

            <xsd:element name="CreateExportSetRequest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="User" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Client" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Password" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Overwrite" type="xsd:boolean" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="ExportType" type="bdt:ExportSetType" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="ExportSetupArgs" type="bdt:ExportSetupArgs" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="CreateExportSetResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Response" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="DeleteExportSetRequest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="User" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Client" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Password" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="ExportSetName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="DeleteExportSetResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Response" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="RetrieveExportSetRequest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="User" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Client" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Password" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="ExportSetName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="ExportSetOwner" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="RetrieveExportSetResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="ExportSetupArgs" type="bdt:ExportSetupArgs" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="RetrieveOptimizationProfileRequest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="User" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Client" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Password" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="OptimizationProfileName" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="OptimizationProfileOwner" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>
			
			<xsd:element name="SubmitRebalanceJobRequest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="User" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Client" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Password" type="xsd:string" minOccurs="1" maxOccurs="1"/>						
                        <xsd:element name="RebalanceJobArgs" type="bdt:RebalanceJobArgs" minOccurs="1" maxOccurs="1"/>        
						<xsd:element name="RebalanceProfileSettingsArgs" type="bdt:RebalanceProfileSettingsArgs" minOccurs="0" maxOccurs="1"/>	
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:complexType name ="RebalanceJobArgs">
                <xsd:sequence>
                    <xsd:element name="IterationName" type="bdt:String64Length" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="AnalysisDate" type="xsd:date" minOccurs="1" maxOccurs="1"/>
                    <xsd:element name="RebalanceProfileName" type="bdt:RebalanceProfileName" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
                <xsd:attribute name="PortfolioName" type="xsd:string" use="required"/>
                <xsd:attribute name="PortfolioOwner" type="xsd:string" use="required"/>
            </xsd:complexType>
			


            <xsd:complexType name ="RebalanceProfileName">
                <xsd:attribute name="ProfileName" type="xsd:string" use="required"/>
                <xsd:attribute name="ProfileOwner" type="xsd:string" use="required"/>
            </xsd:complexType>
			
			
			<xsd:element name="SubmitRebalanceJobResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="Response" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>


            <xsd:complexType  name="RebalanceProfileSettingsArgs">
                <xsd:sequence>
                    <xsd:element name="OptimizationSetting" type="bdt:OptimizationSetting" minOccurs="1" maxOccurs="1"/>
                </xsd:sequence>
            </xsd:complexType>


			<xsd:complexType name="OptimizationSetting">
                    <xsd:sequence>
                        <xsd:element name="OptimizationMethod" type="bdt:OptimizationMethod" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
            </xsd:complexType>



            <!-- <xsd:complexType  name="OptimizationMethod">    Here we have to go for the use of choice element else this wont be able to use for setting\overriding the profile 
			<xsd:choice minOccurs="0" maxOccurs="1">
					<xsd:element name="SingleOptimizationProfile" type="bdt:SingleOptimizationProfile" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="SensitivityOptimizationProfile" type="xsd:string" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="FrontierOptimizationProfile" type="xsd:string" minOccurs="1" maxOccurs="1"/>
					<xsd:element name="MultiPeriodOptimizationProfile" type="xsd:string" minOccurs="1" maxOccurs="1"/>
            </xsd:choice>
			</xsd:complexType> -->
			
			<xsd:complexType  name="OptimizationMethod">    <!--Here we have to go for the use of choice element else this wont be able to use for setting\overriding the profile -->
			<xsd:sequence>
								
					<xsd:element name="SingleOptimizationProfile" type="bdt:SingleOptimizationProfile" minOccurs="0" maxOccurs="1"/>					
            </xsd:sequence>
			</xsd:complexType>			

			<!-- <xsd:complexType name="SingleOptimizationProfile">     Here we have to go for the use of choice element else this wont be able to use for setting\overriding the profile
                <xsd:choice minOccurs="0" maxOccurs="1">
                        <xsd:element name="MaxUtility" type="bdt:MaxUtility" minOccurs="1" maxOccurs="1"/>
						<xsd:element name="RiskTarget" type="xsd:string" minOccurs="1" maxOccurs="1"/>
						<xsd:element name="ReturnTarget" type="xsd:string" minOccurs="1" maxOccurs="1"/>
						<xsd:element name="MaxInformationRatio" type="xsd:string" minOccurs="1" maxOccurs="1"/>
						<xsd:element name="MAxSharpeRatio" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                </xsd:choice>
			</xsd:complexType> -->
			
			<xsd:complexType name="SingleOptimizationProfile">
                <xsd:sequence>
                        				
                        <xsd:element name="MaxUtility" type="bdt:MaxUtility" minOccurs="0" maxOccurs="1"/>						
                </xsd:sequence>
			</xsd:complexType>			

			<xsd:complexType name="MaxUtility">
				<xsd:sequence>
				
				
				<xsd:element name="MaxUtilityConstraints" type="bdt:MaxUtilityConstraints" minOccurs="0" maxOccurs="1"/>
				<xsd:element name="MaxUtilityGeneralSettings" type="bdt:MaxUtilityGeneralSettings" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:complexType>

			<xsd:complexType name="MaxUtilityGeneralSettings">
				<xsd:sequence>
					<xsd:element name="ReferenceSetting" type="bdt:ReferenceSetting" minOccurs="0" maxOccurs="1"/>					
				</xsd:sequence>
			</xsd:complexType>
			
			
			<xsd:complexType name="ReferenceSetting">
				<xsd:sequence>
					<xsd:element name="ReferenceProperties" type="bdt:ReferenceProperties" minOccurs="0" maxOccurs="1"/>					
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="ReferenceProperties">
				<xsd:sequence>
					<xsd:element name="PrimaryBenchmark" type="bdt:ReferencePropertyType" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="SecondaryBenchmark" type="bdt:ReferencePropertyType" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="Market" type="bdt:ReferencePropertyType" minOccurs="0" maxOccurs="1"/>
                    <xsd:element name="Universe" type="bdt:ReferencePropertyType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
				<xsd:attribute name="Currency" type="xsd:string" use="required"/>
			</xsd:complexType>
					

            <xsd:complexType name="ReferencePropertyType">
                <xsd:choice minOccurs="1" maxOccurs="1">                    
					<xsd:element name="Cash" type="xsd:string"/>  
                    <xsd:element name="PortfolioType" type="bdt:PortfolioDataType"/>
                    <xsd:element name="ClearData" type="xsd:boolean"/>
                </xsd:choice>
            </xsd:complexType>
			

			
			<xsd:complexType name="PortfolioDataType">
				<xsd:attribute name="PorfotlioName" type="xsd:string" use="required"/>
				<xsd:attribute name="PorfotlioOwner" type="xsd:string" use="required"/>
			</xsd:complexType>

			<xsd:complexType name="MaxUtilityConstraints">
				<xsd:sequence>
					<xsd:element name="CashSettings" type="bdt:CashSettings" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="TurnoverConstraints" type="bdt:TurnoverConstraints" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="NonCashAssets" type="bdt:NonCashAssets" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="AssetLevelBounds" type="bdt:AssetLevelBounds" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="AssetLevelBounds">
				<xsd:sequence>
					<xsd:element name="LowerBound" type="bdt:AssetLvlBoundType" minOccurs="0" maxOccurs="1"/>					
					<xsd:element name="UpperBound" type="bdt:AssetLvlBoundType" minOccurs="0" maxOccurs="1"/>					
				</xsd:sequence>					
			</xsd:complexType>
			
			<xsd:complexType name="AssetLvlBoundType">
				<xsd:choice minOccurs="1" maxOccurs="1">										
					<xsd:element name="AssetAttribute" type="bdt:TypeAssetAttribute"/>
					<xsd:element name="ClearData" type="xsd:boolean"/>
				</xsd:choice>
			</xsd:complexType>
			
			<xsd:complexType name="TypeAssetAttribute"> 
				<xsd:attribute name="AssetAttributeName" type="xsd:string" use="required"/>
				<xsd:attribute name="AssetAttributeOwner" type="xsd:string" use="required"/>
			</xsd:complexType>
			
			<xsd:complexType name="NonCashAssets">
				<xsd:sequence>
					<xsd:element name="LowerBound" type="bdt:BoundType" minOccurs="0" maxOccurs="1"/>					
					<xsd:element name="UpperBound" type="bdt:BoundType" minOccurs="0" maxOccurs="1"/>					
				</xsd:sequence>					
			</xsd:complexType>
			
			<xsd:complexType name="TurnoverConstraints">
				<xsd:sequence>
					<xsd:element name="MaxTurnover" type="bdt:TurnoverConstraintType" minOccurs="0" maxOccurs="1"/>					
					<xsd:element name="MaxBuyTurnover" type="bdt:TurnoverConstraintType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="MaxSellTurnover" type="bdt:TurnoverConstraintType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
					<xsd:attribute name="UseBaseValue" type="xsd:boolean" use="optional" default="false"/>
			</xsd:complexType>
			
			<xsd:complexType name="TurnoverConstraintType">
			<xsd:sequence>
				<xsd:element name="Value" type="bdt:ValueType" minOccurs="0" maxOccurs="1"/>    <!-- For now I have kept the value to double but later I will make it to MUDatatype -->
				<xsd:element name="SetPriority" type="bdt:PriorityType" minOccurs="0" maxOccurs="1"/>
			</xsd:sequence>
			</xsd:complexType>
			
			<xsd:complexType name="ValueType">
				<xsd:choice minOccurs="1" maxOccurs="1">					
					<xsd:element name="Double" type="xsd:double"/>
					<xsd:element name="PortfolioAttribute" type="bdt:TypePortfolioAttribute"/>
					<xsd:element name="ClearData" type="xsd:boolean"/>
				</xsd:choice>
			</xsd:complexType>
					
			
			<xsd:simpleType name="PriorityType">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="MANDATORY"/>
                    <xsd:enumeration value="SOFT"/>
                </xsd:restriction>
            </xsd:simpleType>
			
			<xsd:complexType name="TypePortfolioAttribute"> 
				<xsd:attribute name="PortfolioAttributeName" type="xsd:string" use="required"/>
				<xsd:attribute name="PortfolioAttributeOwner" type="xsd:string" use="required"/>
			</xsd:complexType>

			<xsd:complexType name="CashSettings">
				<xsd:sequence>
					<xsd:element name="CashInOut" type="bdt:CashInOutType" minOccurs="0" maxOccurs="1"/>					
					<xsd:element name="LowerBound" type="bdt:BoundType" minOccurs="0" maxOccurs="1"/>
					<xsd:element name="UpperBound" type="bdt:BoundType" minOccurs="0" maxOccurs="1"/>
				</xsd:sequence>
					<xsd:attribute name="InvestAllCash" type="xsd:boolean" use="optional" default="false"/>
			</xsd:complexType>
			
			<xsd:complexType name="BoundType">
				<xsd:choice minOccurs="1" maxOccurs="1">	
					<xsd:element name="Double" type="xsd:double"/>
					<xsd:element name="ClearData" type="xsd:boolean"/>
				</xsd:choice>
			</xsd:complexType>

			<xsd:complexType name="CashInOutType">
				<xsd:choice minOccurs="1" maxOccurs="1">					
					<xsd:element name="Double" type="xsd:double"/>									
					<xsd:element name="ClearData" type="xsd:boolean"/>
				</xsd:choice>
			</xsd:complexType>			
			

            <xsd:element name="RetrieveOptimizationProfileResponse">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element ref="bdt:OptimizationSetupArgs" minOccurs="1" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="OptimizationSetupArgs">
                <xsd:complexType>
                    <xsd:sequence>
                        <!-- Essentially a choice of one of the four types below.  However, avoiding use of the choice directive due
                 to how .net will force the client to cast the result object to the appropriate type.  Better to simply have the
                 returned chosen element be non-null and the unchosen elements be null (which is how java handles choice bindings).
                 Cleaner and easier to handle that way.-->
                        <xsd:element name="StandardOptimizationProfile" type="bdt:StandardOptimizationProfile" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="RiskTargetOptimizationProfile" type="bdt:RiskTargetOptimizationProfile" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="LongShortOptimizationProfile" type="bdt:LongShortOptimizationProfile" minOccurs="0" maxOccurs="1"/>
                        <xsd:element name="EfficientFrontierOptimizationProfile" type="bdt:EfficientFrontierOptimizationProfile" minOccurs="0" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
            </xsd:element>

            <xsd:element name="DeleteExportJobScheduleRequest">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="User" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Client" type="xsd:string" minOccurs="1" maxOccurs="1"/>
                        <xsd:element name="Password" type="xsd:string" minOccurs="1" maxOccurs="1"/>
