            <xsd:element name="GetExportJobResponse2ADB">
                <xsd:complexType>
                    <xsd:sequence>
                        <xsd:element name="AttachmentExportJob2ADB" type="bdt:AttachmentType2ADB" minOccurs="0" maxOccurs="1"/>
                    </xsd:sequence>
                </xsd:complexType>
