            <xsd:complexType name="AnalysisSettingsNameOwner">
                <xsd:attribute name="AnalysisSettingsName" type="xsd:string" use="required"/>
                <xsd:attribute name="AnalysisSettingsOwner" type="xsd:string" use="optional"/>
